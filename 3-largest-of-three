public static Integer findLargest(Integer num1, Integer num2, Integer num3) {

	Integer largestNumber;

    if (num1 > num2) {
        if (num1 > num3) {
            largestNumber = num1;
        }else {
            largestNumber = num3;
        }
    }else {
        if (num2 > num3) {
            largestNumber = num2;
        }else {
            largestNumber = num3;
        }
    }
    return largestNumber;
}