public Boolean isKeyAccount(Account a) {
    
    if ((a.Industry == 'Banking' && a.AnnualRevenue >= 600000) || (a.Industry == 'Technology' && a.AnnualRevenue >= 800000) || (a.Industry == 'Retail' && a.AnnualRevenue >= 2000000)) {
        return true;
    }else if (a.Industry != 'Banking' && a.Industry != 'Technology' && a.Industry != 'Retail' && a.AnnualRevenue >= 500000) {
        return true;
    }
    return false;
    
}