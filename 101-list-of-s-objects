public List<sObject> getListOfsObject(List<Account> accounts, List<Contact> contacts) {
    
    List<sObject> sObjList = new List<sObject>();

    if (accounts?.size() > 0) {
        for (sObject acc : accounts) {
            sObjList.add(acc);
        }
    }

    if (contacts?.size() > 0) {
        for (sObject ctt : contacts) {
            sObjList.add(ctt);
        }
    }

    return sObjList;
    
}