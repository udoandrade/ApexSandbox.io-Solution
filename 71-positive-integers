public List<Integer> positiveIntegers(List<Integer> numbers) {
    
    Integer tam = 0;
    List<Integer> positivesOnly = new List<Integer>();

    for (Integer i = 0; i < tam; i++) {
        if (numbers[i] > 0) {
            positivesOnly.add(numbers[i]);
        }
    }

    return positivesOnly;
}