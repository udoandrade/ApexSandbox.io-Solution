public class Stack {
    
    List<Object> objs = new List<Object>();
    
    public void push(Object obj) {
        objs.add(obj);
    }
    
    public Object pop() {
        if (!isEmpty()) {
            Object obj = objs.remove(objs.size()-1);
            return obj;
        }
        return null;
    }
    
    public Integer size() {
        return objs.size();
    }
    
    public Object peek(){
        return objs[objs.size()-1];
    }
    
    public Boolean isEmpty() {
        return objs.isEmpty();
    }
}